{
  "name": "chirashi-event-emitter",
  "version": "1.0.1",
  "description": "Simplest event emitter",
  "keywords": [
    "events",
    "emitter",
    "event-emitter",
    "listener"
  ],
  "main": "./dist/chirashi-event-emitter.common.js",
  "module": "./dist/chirashi-event-emitter.mjs",
  "jsnext:main": "./dist/chirashi-event-emitter.mjs",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "node build/build.js",
    "dev:test": "NODE_ENV=test karma start build/karma.dev.conf.js",
    "test:unit": "NODE_ENV=test karma start build/karma.unit.conf.js",
    "test:cover": "NODE_ENV=test karma start build/karma.cover.conf.js",
    "test": "npm run lint && npm run test:cover",
    "analyze": "bash build/analyze.sh",
    "lint": "eslint ./lib ./test",
    "docs": "documentation build ./lib -f md -o docs/index.md --github",
    "release": "bash build/release.sh"
  },
  "homepage": "https://chirashijs.org",
  "author": {
    "name": "Alex Toudic",
    "mail": "alex@chirashijs.org",
    "url": "https://alextoudic.xyz"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chirashijs/chirashi-event-emitter.git"
  },
  "bugs": {
    "url": "https://github.com/chirashijs/chirashi/issues"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gzip-size": "^3.0.0",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^2.0.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "uglify-js": "^2.8.22",
    "webpack": "^2.4.1"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
